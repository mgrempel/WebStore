<div>
  <h1>Home page</h1>

  <%= link_to 'Sign up', new_user_registration_path %>

  <div id="Categories">
    <h2>All Categories</h2>
    <table>
      <% @categories.each do |category| %>
        <tr>
          <td><%= link_to category.name, category_path(category) %></td>
        </tr>
      <% end %>
    </table>
  </div>
  <div id="Products">
    <h2>All Items</h2>
    <%= render "layouts/items_table" %>
  </div>
</div>
